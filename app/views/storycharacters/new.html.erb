<%= render "layouts/title_header", title: "New Character " %>
<%= simple_form_for([@story, @story_character], html: { 'data-parsley-validate': true }) do |f| %>
  <%= render "form", f: f %>
  <div class="form-actions pb-2">
    <%= f.button :submit, "Create Character", class: 'btn-dating' %>
  </div>
<% end %>